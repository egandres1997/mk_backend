{
	"apps": [
		{
			"name": "MK API",
			"script": "server.js",
			"watch": true,
			"ignore_watch": [
				"logs",
				"node_modules"
			],
			"instance_var": "1",
			"env": {
				"NODE_ENV": "development",
				"PORT": 3000,
				"DB_CONNECTION": "mysql://root:root@database:3306/tr_database"
			},
			"error_file": "./logs/error.log",
			"out_file": "./logs/out.log",
			"merge_logs": true
		}
	]
}