{
  "name": "mk_backend",
  "version": "1.0.0",
  "description": "MK Backend",
  "main": "server.js",
  "scripts": {
    "start": "NODE_ENV=production node server.js",
    "dev": "cross-env NODE_ENV=development nodemon server.js",
    "dev:pm2": "rm -rf ./logs && cross-env NODE_ENV=development pm2 start ./config/pm2-process.json --only mk_backend_dev",
    "start:pm2": "rm -rf ./logs && NODE_ENV=production pm2 start ./config/pm2-process.json --only mk_backend_prod",
    "lint": "standard",
    "migrate": "cross-env NODE_ENV=development && sequelize db:migrate",
    "reset_dev": "cross-env NODE_ENV=development && sequelize db:migrate:undo:all && sequelize db:migrate && sequelize db:seed:all"
  },
  "author": "Enzo Garc√≠a",
  "license": "MIT",
  "dependencies": {
    "bcrypt": "^3.0.2",
    "body-parser": "1.8.1",
    "cors": "^2.8.4",
    "dotenv": "^6.1.0",
    "express": "4.9.0",
    "express-validator": "^5.3.0",
    "file-system": "^2.2.2",
    "jsonwebtoken": "^8.3.0",
    "jwt-simple": "^0.5.5",
    "moment": "^2.22.2",
    "mysql": "^2.16.0",
    "mysql2": "^1.6.4",
    "sequelize": "^4.13.8",
    "sequelize-cli": "^5.4.0"
  },
  "devDependencies": {
    "cross-env": "^5.2.0",
    "nodemon": "^1.18.5",
    "standard": "^12.0.1"
  }
}
